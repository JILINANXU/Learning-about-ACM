#include <bits/stdc++.h>
using namespace std;
#define For(i,a,b) for(int i=(a);i<=(int)(b);i++)

const int N = 1e5+10;

int n, a[N], p[N];

void Msort(int l, int r)
{
	if(l >= r) return;
	
	int mid = l + r;
	mid >>= 1;
	
	Msort(l, mid);
	Msort(mid+1, r);
	
	int i = l, j = mid + 1, k = l;
	
	while(i<=mid && j<=r)
	{
		if(a[i] <= a[j])
		{
			p[k++] = a[i++];
		}
		else
		{
			p[k++] = a[j++];
		}
	}
	
	while(i <= mid)
	{
		p[k++] = a[i++];
	}
	
	while(j <= r)
	{
		p[k++] = a[j++];
	}
	
	For(z, l, r)
	{
		a[z] = p[z];
	}
}

int main()
{
	scanf("%d",&n);
	
	For(i, 1, n)
	{
		scanf("%d",&a[i]);
	}
	
	Msort(1, n);
	
	For(i, 1, n)
	{
		printf("%d ",a[i]);
	}
	
	return 0;
}
