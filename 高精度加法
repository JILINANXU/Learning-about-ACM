#include "bits/stdc++.h"
using namespace std;
int a[5000],b[5000],c[5000];
char s1[5000],s2[5000];
int main()
{
	scanf("%s%s",s1,s2);
	a[0]=strlen(s1);
	b[0]=strlen(s2);
	c[0]=max(a[0],b[0])+1;
	for(int i=a[0];i>=1;i--)
	{
		a[i]=s1[a[0]-i]-'0';
	}
	for(int i=b[0];i>=1;i--)
	{
		b[i]=s2[b[0]-i]-'0';
	}
	for(int i=1;i<=c[0];i++)
	{
		c[i]+=a[i]+b[i];
		c[i+1]+=c[i]/10;
		c[i]%=10;
	}
	while(c[0]>1&&c[c[0]]==0)
	{
		c[0]--;
	}
	for(int i=c[0];i>=1;i--)
	{
		printf("%d",c[i]);
	}
	return 0;
}
