#include <bits/stdc++.h>
using namespace std;
#define For(i,a,b) for(int i=(a);i<=(int)(b);i++)
priority_queue<int,vector<int>,greater<int> > q;
int op,n;
int main(){
	scanf("%d",&n);
	For(i,1,n){
		scanf("%d",&op);
		if(op==1){
			scanf("%d",&op);
			q.push(op);
		}else if(op==2){
			printf("%d\n",q.top());
		}else if(op==3){
			q.pop();
		}
	}
}
